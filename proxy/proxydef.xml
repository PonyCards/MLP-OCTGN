	<!-- Default template -->
	<template src="proxy/proxyImages/unknownCard.jpg" default="True">
		<matches>
			<match name="dummy" value="dummy" />
		</matches>
		<overlayblocks>
			<link block="dummyOverlay" />
		</overlayblocks>
		<textblocks>
			<link block="default">
				<property name="Name" />
			</link>
			<link block="dummyText" />
		</textblocks>
	</template>
	
	<!-- Event Card template -->
	<template src="proxy/proxyImages/cardTemplates/Event_Template.png" default="False">
		<matches>
		<match name="Type" value="Event" />
		</matches>

		<overlayblocks>

		<conditional>
			<if property="PlayRequiredElement" contains="Loyalty">
				<link block="requiredPowerLoyalty" /> 
			</if>
		</conditional>

		<conditional>
			<if property="PlayRequiredElement" contains="Kindness">
				<link block="requiredPowerKindness" /> 
			</if>
		</conditional>			

		<conditional>
			<if property="PlayRequiredElement" contains="Magic">
				<link block="requiredPowerMagic" /> 
			</if>
		</conditional>
		
		<conditional>
			<if property="PlayRequiredElement" contains="Generosity">
				<link block="requiredPowerGenerosity" /> 
			</if>
		</conditional>

		<conditional>
			<if property="PlayRequiredElement" contains="Honesty">
				<link block="requiredPowerHonesty" /> 
			</if>
		</conditional>

		<conditional>
			<if property="PlayRequiredElement" contains="Laughter">
				<link block="requiredPowerLaughter" /> 
			</if>
		</conditional>		

		<link block="dummyOverlay" />
		</overlayblocks>

		<textblocks>

		<link block="cardCost">
			<property name="Cost" />
		</link>

		<conditional>
			<if property="PlayRequiredPower" value="">
				<link block="dummyOverlay" /> 
			</if>
			<else>
				<link block="cardRequiredPower">
					<property name="PlayRequiredPower" />
				</link> 
			</else>
		</conditional>


		<link block="eventCardPower">
			<property name="Power" />
		</link>

		<link block="cardType">
			<property name="Type" />
		</link>

		<link block="cardTraits">
			<property name="Traits" />
		</link>

		<link block="cardKeywords">
			<property name="Keywords" />
		</link>		

		<link block="eventCardName">
			<property name="Name" />
		</link>

		<link block="cardText">
			<property name="Text" />
		</link>
	</textblocks>
	</template>
	
